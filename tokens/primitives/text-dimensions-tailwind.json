{
  "letter-spacing": {
    "tracking-tighter": {
      "$type": "letterSpacing",
      "$value": "-4%",
      "$description": "-0.5em"
    },
    "tracking-tight": {
      "$type": "letterSpacing",
      "$value": "-2%",
      "$description": "-0.025em"
    },
    "tracking-normal": {
      "$type": "letterSpacing",
      "$value": "0%",
      "$description": "0em"
    },
    "tracking-wide": {
      "$type": "letterSpacing",
      "$value": "4%",
      "$description": "0.025em"
    }
  },
  "setname-tw-type": {
    "$type": "other",
    "$value": "tw_typography"
  },
  "notes": {
    "font-size": {
      "customization": {
        "$type": "other",
        "$value": "default",
        "$description": "this token set has the default values of font sizes listed on the tailwind website. If you want to use these, you'll need to swap out the line high of choice in your typography styles by changing the reference tokens. "
      }
    }
  },
  "font-size": {
    "text-xs": {
      "$type": "fontSizes",
      "$value": "0.75rem",
      "$description": "12px"
    },
    "text-sm": {
      "$type": "fontSizes",
      "$value": "0.875rem",
      "$description": "14px"
    },
    "text-base": {
      "$type": "fontSizes",
      "$value": "1rem",
      "$description": "16px Smallest body size"
    },
    "text-lg": {
      "$type": "fontSizes",
      "$value": "1.125rem",
      "$description": "18px"
    },
    "text-xl": {
      "$type": "fontSizes",
      "$value": "1.25rem",
      "$description": "20px"
    },
    "text-2xl": {
      "$type": "fontSizes",
      "$value": "1.5rem",
      "$description": "24px"
    },
    "text-3xl": {
      "$type": "fontSizes",
      "$value": "1.875rem",
      "$description": "30px"
    },
    "text-4xl": {
      "$type": "fontSizes",
      "$value": "2.25rem",
      "$description": "36px"
    },
    "text-5xl": {
      "$type": "fontSizes",
      "$value": "3rem",
      "$description": "48px"
    },
    "text-6xl": {
      "$type": "fontSizes",
      "$value": "3.75rem",
      "$description": "60px"
    },
    "text-7xl": {
      "$type": "fontSizes",
      "$value": "4.5rem",
      "$description": "72px"
    },
    "text-8xl": {
      "$type": "fontSizes",
      "$value": "6rem",
      "$description": "96px"
    },
    "text-9xl": {
      "$type": "fontSizes",
      "$value": "8rem",
      "$description": "128px"
    }
  },
  "relative": {
    "line-height": {
      "leading-none": {
        "$type": "lineHeights",
        "$value": "100%",
        "$description": "100% needs to be transformed as value 1 to match CSS"
      },
      "leading-tight": {
        "$type": "lineHeights",
        "$value": "125%",
        "$description": "125% needs to be transformed as value 1.25 to match CSS"
      },
      "leading-snug": {
        "$type": "lineHeights",
        "$value": "138%",
        "$description": "138% needs to be transformed as value 1.375 to match CSS"
      },
      "leading-normal": {
        "$type": "lineHeights",
        "$value": "150%",
        "$description": "150% needs to be transformed as value 1.5 to match CSS"
      },
      "leading-relaxed": {
        "$type": "lineHeights",
        "$value": "163%",
        "$description": "163% needs to be transformed as value 1.625 to match CSS"
      },
      "leading-loose": {
        "$type": "lineHeights",
        "$value": "200%",
        "$description": "200% needs to be transformed as value 2 to match CSS"
      }
    }
  }
}