{
  "component": {
    "button": {
      "base": {
        "radius": {
          "$type": "dimension",
          "$value": "{dimensions.border-radius.rounded-default}"
        },
        "borderWidth": {
          "$type": "dimension",
          "$value": "{theme-spacing.border-width.none}"
        }
      },
      "variant": {
        "default": {
          "bg": {
            "default": {
              "$type": "color",
              "$value": "{theme-colors.primary.default}"
            },
            "hover": {
              "$type": "color",
              "$value": "{theme-colors.primary.hover}"
            },
            "active": {
              "$type": "color",
              "$value": "{theme-colors.primary.hover}"
            },
            "disabled": {
              "$type": "color",
              "$value": "{theme-colors.background.disabled}"
            }
          },
          "text": {
            "default": {
              "$type": "color",
              "$value": "{theme-colors.foreground.default}"
            },
            "disabled": {
              "$type": "color",
              "$value": "{theme-colors.foreground.disabled}"
            }
          },
          "border": {
            "default": {
              "$type": "color",
              "$value": "{theme-colors.background.transparent}"
            }
          }
        },
        "secondary": {
          "bg": {
            "default": {
              "$type": "color",
              "$value": "{theme-colors.secondary.default}"
            },
            "hover": {
              "$type": "color",
              "$value": "{theme-colors.secondary.hover}"
            },
            "active": {
              "$type": "color",
              "$value": "{theme-colors.secondary.hover}"
            },
            "disabled": {
              "$type": "color",
              "$value": "{theme-colors.background.disabled}"
            }
          },
          "text": {
            "default": {
              "$type": "color",
              "$value": "{theme-colors.foreground.contrast}"
            },
            "disabled": {
              "$type": "color",
              "$value": "{theme-colors.foreground.disabled}"
            }
          },
          "border": {
            "default": {
              "$type": "color",
              "$value": "{theme-colors.background.transparent}"
            }
          }
        },
        "destructive": {
          "bg": {
            "default": {
              "$type": "color",
              "$value": "{theme-colors.danger.default}"
            },
            "hover": {
              "$type": "color",
              "$value": "{theme-colors.danger.hover}"
            },
            "active": {
              "$type": "color",
              "$value": "{theme-colors.danger.hover}"
            },
            "disabled": {
              "$type": "color",
              "$value": "{theme-colors.danger.disabled}"
            }
          },
          "text": {
            "default": {
              "$type": "color",
              "$value": "{theme-colors.foreground.contrast}"
            },
            "disabled": {
              "$type": "color",
              "$value": "{theme-colors.foreground.disabled}"
            }
          },
          "border": {
            "default": {
              "$type": "color",
              "$value": "{theme-colors.background.transparent}"
            }
          }
        },
        "outline": {
          "bg": {
            "default": {
              "$type": "color",
              "$value": "{theme-colors.background.transparent}"
            },
            "hover": {
              "$type": "color",
              "$value": "{theme-colors.background.subtle}"
            },
            "active": {
              "$type": "color",
              "$value": "{theme-colors.background.subtle}"
            },
            "disabled": {
              "$type": "color",
              "$value": "{theme-colors.background.disabled}"
            }
          },
          "text": {
            "default": {
              "$type": "color",
              "$value": "{theme-colors.foreground.default}"
            },
            "hover": {
              "$type": "color",
              "$value": "{theme-colors.border.active}"
            },
            "disabled": {
              "$type": "color",
              "$value": "{theme-colors.foreground.disabled}"
            }
          },
          "border": {
            "default": {
              "$type": "color",
              "$value": "{theme-colors.border.strong}"
            },
            "disabled": {
              "$type": "color",
              "$value": "{theme-colors.foreground.disabled}"
            }
          },
          "borderWidth": {
            "$type": "dimension",
            "$value": "{dimensions.border-width.2}"
          }
        },
        "ghost": {
          "bg": {
            "default": {
              "$type": "color",
              "$value": "{theme-colors.background.transparent}"
            },
            "hover": {
              "$type": "color",
              "$value": "{theme-colors.background.subtle}"
            },
            "active": {
              "$type": "color",
              "$value": "{theme-colors.background.subtle}"
            },
            "disabled": {
              "$type": "color",
              "$value": "{theme-colors.background.disabled}"
            }
          },
          "text": {
            "default": {
              "$type": "color",
              "$value": "{theme-colors.foreground.default}"
            },
            "disabled": {
              "$type": "color",
              "$value": "{theme-colors.foreground.disabled}"
            }
          },
          "border": {
            "default": {
              "$type": "color",
              "$value": "{theme-colors.background.transparent}"
            }
          }
        },
        "link": {
          "bg": {
            "default": {
              "$type": "color",
              "$value": "{theme-colors.background.transparent}"
            },
            "hover": {
              "$type": "color",
              "$value": "{theme-colors.background.transparent}"
            },
            "active": {
              "$type": "color",
              "$value": "{theme-colors.background.transparent}"
            },
            "disabled": {
              "$type": "color",
              "$value": "{theme-colors.background.transparent}"
            }
          },
          "text": {
            "default": {
              "$type": "color",
              "$value": "{theme-colors.primary.default}"
            },
            "hover": {
              "$type": "color",
              "$value": "{theme-colors.primary.hover}"
            },
            "active": {
              "$type": "color",
              "$value": "{theme-colors.primary.hover}"
            },
            "disabled": {
              "$type": "color",
              "$value": "{theme-colors.foreground.disabled}"
            }
          },
          "border": {
            "default": {
              "$type": "color",
              "$value": "{theme-colors.background.transparent}"
            }
          },
          "textStyle": {
            "$type": "typography",
            "$value": "{brand_typography.text-base.underline}"
          }
        }
      },
      "size": {
        "default": {
          "padding": {
            "x": { "$type": "dimension", "$value": "{theme-spacing.md}" },
            "y": { "$type": "dimension", "$value": "{theme-spacing.xs}" }
          },
          "textStyle": { "$type": "typography", "$value": "{brand_typography.text-base.medium}" },
          "gap": { "$type": "dimension", "$value": "{theme-spacing.gap.sm}" },
          "icon": { "size": { "$type": "sizing", "$value": "{dimensions.fixed.6}" } }
        },
        "sm": {
          "padding": {
            "x": { "$type": "dimension", "$value": "{theme-spacing.sm}" },
            "y": { "$type": "dimension", "$value": "{theme-spacing.2xs}" }
          },
          "textStyle": { "$type": "typography", "$value": "{brand_typography.text-sm.medium}" },
          "gap": { "$type": "dimension", "$value": "{theme-spacing.gap.xs}" },
          "icon": { "size": { "$type": "sizing", "$value": "{dimensions.fixed.5}" } }
        },
        "lg": {
          "padding": {
            "x": { "$type": "dimension", "$value": "{theme-spacing.lg}" },
            "y": { "$type": "dimension", "$value": "{theme-spacing.sm}" }
          },
          "textStyle": { "$type": "typography", "$value": "{brand_typography.text-lg.medium}" },
          "gap": { "$type": "dimension", "$value": "{theme-spacing.gap.md}" },
          "icon": { "size": { "$type": "sizing", "$value": "{dimensions.fixed.7}" } }
        },
        "icon": {
          "padding": {
            "default": { "$type": "dimension", "$value": "{theme-spacing.xs}" }
          },
          "textStyle": { "$type": "typography", "$value": "{brand_typography.text-base.medium}" },
          "gap": { "$type": "dimension", "$value": "{theme-spacing.gap.none}" },
          "icon": { "size": { "$type": "sizing", "$value": "{dimensions.fixed.6}" } }
        }
      }
    },
    "input": {
      "bg": {
        "default": { "$type": "color", "$value": "{theme-colors.background.default}" },
        "disabled": { "$type": "color", "$value": "{theme-colors.background.disabled}" }
      },
      "border": {
        "default": { "$type": "color", "$value": "{theme-colors.border.subtle}" },
        "disabled": { "$type": "color", "$value": "{theme-colors.border.disabled}" },
        "error": { "$type": "color", "$value": "{theme-colors.danger.default}" }
      },
      "text": {
        "default": { "$type": "color", "$value": "{theme-colors.foreground.default}" },
        "placeholder": { "$type": "color", "$value": "{theme-colors.foreground.placeholder}" },
        "disabled": { "$type": "color", "$value": "{theme-colors.foreground.disabled}" }
      },
      "radius": { "$type": "dimension", "$value": "{theme-tokens.radius}" },
      "borderWidth": { "$type": "dimension", "$value": "{theme-spacing.border-width.default}" },
      "padding": {
        "x": { "$type": "dimension", "$value": "{theme-spacing.md}" },
        "y": { "$type": "dimension", "$value": "{theme-spacing.sm}" }
      },
      "textStyle": { "$type": "typography", "$value": "{brand_typography.text-base.medium}" }
    },
    "textarea": {
      "bg": { "$type": "color", "$value": "{component.input.bg.default}" },
      "border": { "$type": "color", "$value": "{component.input.border.default}" },
      "text": { "$type": "color", "$value": "{component.input.text.default}" },
      "placeholder": { "$type": "color", "$value": "{component.input.text.placeholder}" },
      "radius": { "$type": "dimension", "$value": "{component.input.radius}" },
      "borderWidth": { "$type": "dimension", "$value": "{component.input.borderWidth}" },
      "padding": {
        "x": { "$type": "dimension", "$value": "{component.input.padding.x}" },
        "y": { "$type": "dimension", "$value": "{theme-spacing.md}" }
      },
      "textStyle": { "$type": "typography", "$value": "{component.input.textStyle}" }
    },
    "checkbox": {
      "root": {
        "border": { "$type": "color", "$value": "{theme-colors.border.strong}" },
        "radius": { "$type": "dimension", "$value": "{theme-spacing.radius.sm}" },
        "size": { "$type": "dimension", "$value": "{dimensions.fixed.4}" },
        "bg": {
            "default": { "$type": "color", "$value": "{theme-colors.background.default}" },
            "checked": { "$type": "color", "$value": "{theme-colors.primary.default}" }
        }
      },
      "indicator": {
        "color": { "$type": "color", "$value": "{theme-colors.foreground.inverse}" }
      }
    },
    "radio": {
      "root": {
        "border": { "$type": "color", "$value": "{theme-colors.border.strong}" },
        "radius": { "$type": "dimension", "$value": "{dimensions.border-radius.rounded-full}" },
        "size": { "$type": "dimension", "$value": "{dimensions.fixed.4}" },
        "bg": { "$type": "color", "$value": "{theme-colors.background.default}" }
      },
      "indicator": {
        "color": { "$type": "color", "$value": "{theme-colors.primary.default}" },
        "size": { "$type": "dimension", "$value": "{dimensions.fixed.2}" }
      }
    },
    "switch": {
      "root": {
        "bg": {
            "default": { "$type": "color", "$value": "{theme-colors.background.elevated}" },
            "checked": { "$type": "color", "$value": "{theme-colors.primary.default}" }
        },
        "radius": { "$type": "dimension", "$value": "{dimensions.border-radius.rounded-full}" },
        "width": { "$type": "dimension", "$value": "{dimensions.fixed.11}" },
        "height": { "$type": "dimension", "$value": "{dimensions.fixed.6}" }
      },
      "thumb": {
        "bg": { "$type": "color", "$value": "{theme-colors.background.default}" },
        "radius": { "$type": "dimension", "$value": "{dimensions.border-radius.rounded-full}" },
        "size": { "$type": "dimension", "$value": "{dimensions.fixed.5}" }
      }
    },
    "label": {
      "textStyle": { "$type": "typography", "$value": "{brand_typography.text-sm.medium}" },
      "color": { "$type": "color", "$value": "{theme-colors.foreground.default}" }
    },
    "avatar": {
      "root": {
        "radius": { "$type": "dimension", "$value": "{dimensions.border-radius.rounded-full}" },
        "overflow": { "$type": "other", "$value": "hidden" }
      },
      "fallback": {
        "bg": { "$type": "color", "$value": "{theme-colors.background.elevated}" },
        "text": { "$type": "color", "$value": "{theme-colors.foreground.muted}" }
      }
    },
    "badge": {
      "base": {
        "radius": { "$type": "dimension", "$value": "{theme-spacing.radius.full}" },
        "borderWidth": { "$type": "dimension", "$value": "{theme-spacing.border-width.none}" },
        "padding": {
            "x": { "$type": "dimension", "$value": "{theme-spacing.sm}" },
            "y": { "$type": "dimension", "$value": "{theme-spacing.2xs}" }
        },
        "textStyle": { "$type": "typography", "$value": "{brand_typography.text-sm.semibold}" }
      },
      "variant": {
        "default": {
          "bg": { "$type": "color", "$value": "{theme-colors.primary.default}" },
          "text": { "$type": "color", "$value": "{theme-colors.foreground.inverse}" }
        },
        "secondary": {
          "bg": { "$type": "color", "$value": "{theme-colors.secondary.default}" },
          "text": { "$type": "color", "$value": "{theme-colors.foreground.contrast}" }
        },
        "destructive": {
          "bg": { "$type": "color", "$value": "{theme-colors.danger.default}" },
          "text": { "$type": "color", "$value": "{theme-colors.foreground.inverse}" }
        },
        "outline": {
          "bg": { "$type": "color", "$value": "{theme-colors.background.transparent}" },
          "text": { "$type": "color", "$value": "{theme-colors.foreground.default}" },
          "border": { "$type": "color", "$value": "{theme-colors.border.default}" },
          "borderWidth": { "$type": "dimension", "$value": "{dimensions.border-width.default}" }
        }
      }
    },
    "card": {
      "bg": { "$type": "color", "$value": "{theme-colors.background.default}" },
      "border": { "$type": "color", "$value": "{theme-colors.border.subtle}" },
      "borderWidth": { "$type": "dimension", "$value": "{theme-spacing.border-width.default}" },
      "radius": { "$type": "dimension", "$value": "{theme-spacing.radius.lg}" },
      "padding": { "$type": "dimension", "$value": "{theme-spacing.lg}" },
      "text": { "$type": "color", "$value": "{theme-colors.foreground.default}" }
    },
    "popover": {
      "bg": { "$type": "color", "$value": "{theme-colors.background.default}" },
      "border": { "$type": "color", "$value": "{theme-colors.border.subtle}" },
      "radius": { "$type": "dimension", "$value": "{theme-spacing.radius.md}" },
      "padding": { "$type": "dimension", "$value": "{theme-spacing.md}" },
      "shadow": { "$type": "boxShadow", "$value": "{tw_effects.shadow.md}" }
    },
    "menuBase": {
      "content": {
        "bg": { "$type": "color", "$value": "{theme-colors.background.default}" },
        "border": { "$type": "color", "$value": "{theme-colors.border.subtle}" },
        "radius": { "$type": "dimension", "$value": "{theme-spacing.radius.md}" },
        "padding": { "$type": "dimension", "$value": "{theme-spacing.2xs}" },
        "shadow": { "$type": "boxShadow", "$value": "{tw_effects.shadow.lg}" }
      },
      "item": {
        "bg": {
            "default": { "$type": "color", "$value": "{theme-colors.background.transparent}" },
            "hover": { "$type": "color", "$value": "{theme-colors.background.subtle}" }
        },
        "text": { "$type": "color", "$value": "{theme-colors.foreground.default}" },
        "radius": { "$type": "dimension", "$value": "{theme-spacing.radius.sm}" },
        "padding": {
            "y": { "$type": "dimension", "$value": "{theme-spacing.xs}" },
            "x": { "$type": "dimension", "$value": "{theme-spacing.sm}" }
        }
      },
      "separator": {
        "bg": { "$type": "color", "$value": "{theme-colors.border.subtle}" },
        "height": { "$type": "dimension", "$value": "{dimensions.fixed.px}" }
      },
      "label": {
         "textStyle": { "$type": "typography", "$value": "{brand_typography.text-sm.semibold}" },
         "padding": {
            "y": { "$type": "dimension", "$value": "{theme-spacing.xs}" },
            "x": { "$type": "dimension", "$value": "{theme-spacing.sm}" }
         }
      }
    },
    "dropdownMenu": {
      "content": {
         "bg": { "$type": "color", "$value": "{component.menuBase.content.bg}" },
         "border": { "$type": "color", "$value": "{component.menuBase.content.border}" },
         "radius": { "$type": "dimension", "$value": "{component.menuBase.content.radius}" },
         "padding": { "$type": "dimension", "$value": "{component.menuBase.content.padding}" },
         "shadow": { "$type": "boxShadow", "$value": "{component.menuBase.content.shadow}" }
      },
      "item": {
         "bg": { "$type": "color", "$value": "{component.menuBase.item.bg.default}" },
         "bgHover": { "$type": "color", "$value": "{component.menuBase.item.bg.hover}" },
         "radius": { "$type": "dimension", "$value": "{component.menuBase.item.radius}" },
         "padding": { "$type": "dimension", "$value": "{component.menuBase.item.padding.x}" }
      }
    },
    "select": {
       "trigger": {
          "border": { "$type": "color", "$value": "{component.input.border.default}" },
          "radius": { "$type": "dimension", "$value": "{component.input.radius}" },
          "padding": { "$type": "dimension", "$value": "{component.input.padding.x}" },
          "height": { "$type": "dimension", "$value": "{dimensions.fixed.10}" }
       },
       "content": { "$type": "other", "$value": "{component.menuBase.content}" },
       "item": { "$type": "other", "$value": "{component.menuBase.item}" }
    },
    "menubar": {
       "root": {
          "bg": { "$type": "color", "$value": "{theme-colors.background.default}" },
          "border": { "$type": "color", "$value": "{theme-colors.border.subtle}" },
          "radius": { "$type": "dimension", "$value": "{theme-spacing.radius.md}" }
       },
       "trigger": {
          "bgHover": { "$type": "color", "$value": "{theme-colors.background.subtle}" }
       },
       "content": { "$type": "other", "$value": "{component.menuBase.content}" },
       "item": { "$type": "other", "$value": "{component.menuBase.item}" }
    },
    "navigationMenu": {
       "root": {
          "bg": { "$type": "color", "$value": "{theme-colors.background.default}" }
       },
       "content": { "$type": "other", "$value": "{component.menuBase.content}" }
    },
    "tabs": {
      "list": {
        "bg": { "$type": "color", "$value": "{theme-colors.background.elevated}" },
        "radius": { "$type": "dimension", "$value": "{theme-spacing.radius.lg}" },
        "padding": { "$type": "dimension", "$value": "{theme-spacing.xs}" }
      },
      "trigger": {
        "bg": {
            "default": { "$type": "color", "$value": "{theme-colors.background.transparent}" },
            "active": { "$type": "color", "$value": "{theme-colors.background.default}" }
        },
        "text": {
            "default": { "$type": "color", "$value": "{theme-colors.foreground.muted}" },
            "active": { "$type": "color", "$value": "{theme-colors.foreground.default}" }
        },
        "radius": { "$type": "dimension", "$value": "{theme-spacing.radius.md}" }
      },
      "content": {
         "border": { "$type": "color", "$value": "{theme-colors.border.subtle}" },
         "radius": { "$type": "dimension", "$value": "{theme-spacing.radius.lg}" },
         "padding": { "$type": "dimension", "$value": "{theme-spacing.lg}" }
      }
    },
    "accordion": {
      "item": {
         "border": { "$type": "color", "$value": "{theme-colors.border.subtle}" }
      },
      "trigger": {
         "text": { "$type": "color", "$value": "{theme-colors.foreground.default}" },
         "textStyle": { "$type": "typography", "$value": "{brand_typography.text-lg.medium}" },
         "padding": { "$type": "dimension", "$value": "{theme-spacing.md}" }
      },
      "content": {
         "text": { "$type": "color", "$value": "{theme-colors.foreground.default}" },
         "padding": { "$type": "dimension", "$value": "{theme-spacing.md}" }
      }
    },
    "separator": {
      "bg": { "$type": "color", "$value": "{theme-colors.border.subtle}" },
      "size": { "$type": "dimension", "$value": "{dimensions.fixed.px}" }
    },
    "dialog": {
      "overlay": {
         "bg": { "$type": "color", "$value": "#000000" },
         "opacity": { "$type": "other", "$value": "0.8" }
      },
      "content": {
         "bg": { "$type": "color", "$value": "{theme-colors.background.default}" },
         "border": { "$type": "color", "$value": "{theme-colors.border.subtle}" },
         "radius": { "$type": "dimension", "$value": "{theme-spacing.radius.xl}" },
         "padding": { "$type": "dimension", "$value": "{theme-spacing.xl}" },
         "shadow": { "$type": "boxShadow", "$value": "{tw_effects.shadow.2xl}" }
      }
    }
  }
}
